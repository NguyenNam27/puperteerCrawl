function fn_get_notification_v2(){var ms=$.cookie("nkmkey_v2");if(typeof ms!="undefined"&&ms!=null&&ms!=""){var s=ms.split(",");var mess;for(var x in s){mess=$.cookie("message_"+s[x]);if(typeof mess!="undefined"&&mess!=null&&mess!=""){mess=JSON.parse(mess);var title=mess.title;if(typeof title!="undefined"&&$.isArray(title)){title=mess.title[0];}
