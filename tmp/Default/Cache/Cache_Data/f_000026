function getCouponHunter(){$(".phone_warning_required").hide();$(".email_warning_required").hide();var _price=$('.price_hunter').val();var _customerPhone=$('.phone_sanvoucher').val();var _product_id=$('#productid_voucher').val();var _CustomerMail=$('.mail_sanvoucher').val();if(_customerPhone==""){$(".phone_warning_required").show();$(".phone_sanvoucher").css("border","1px solid #e73435");return false;}else{$(".phone_sanvoucher").css("border","1px solid #d4d7d9");}
if(_CustomerMail==""){$(".email_warning_required").show();$(".mail_sanvoucher").css("border","1px solid #e73435");return false;}else{$(".mail_sanvoucher").css("border","1px solid #d4d7d9");}
