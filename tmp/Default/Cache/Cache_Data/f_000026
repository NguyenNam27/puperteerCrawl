var CallAjax={load_warehouses:function(url,product_id,isInit){var selState="";var states=[];var cookie_nkarea=$.cookie('nkarea');if(cookie_nkarea!=undefined&&(/^\d+$/.test(cookie_nkarea))&&cookie_nkarea.length==5){selState=cookie_nkarea.substring(2);}else{selState="001";}
