if(_phone_apply_voucher==""){$(".phone_warning_required_ap").show();$(".phone_apply_voucher").css("border","1px solid #e73435");return false;}else{$(".phone_warning_required_ap").hide();$(".phone_apply_voucher").css("border","1px solid #d4d7d9");}
if(_email_apply_voucher==""){$(".email_warning_required_ap").show();$(".email_apply_voucher").css("border","1px solid #e73435");return false;}else{$(".email_warning_required_ap").hide();$(".email_apply_voucher").css("border","1px solid #d4d7d9");}
