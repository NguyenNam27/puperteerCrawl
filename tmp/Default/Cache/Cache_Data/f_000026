$(document).ready(function(){fn_pdp_load_flash_sales();fn_flash_sales_boot();});function fn_flash_sales_boot(is_recall,recall_fs_id){is_recall=is_recall||false;recall_fs_id=recall_fs_id||'xxx';if($(".nk-fs-2019").length>0){$(".nk-fs-2019").each(function(){var fs_id=$(this).attr("id");if(is_recall==true){setTimeout(function(){fn_flash_sales_load_detail(recall_fs_id);},3000);}else{fn_flash_sales_load_detail(fs_id);}});}}
function fn_flash_sales_load_detail(fs_id){var $fs=$("#"+fs_id);var num=$("#num_col_from_block").val();var _data={fs_product_number_in_each_slide:2};if($(".nks_fs_pdp").length==1){var $fs_detail=$(".nks_fs_pdp").eq(0);var fs_product_id=$fs_detail.data("product-id");_data['not_product_id']=fs_product_id;}
