fn_flash_sales_boot(true,fs_id);});$fs.find(".col-flash .owl-carousel").each(function(){var el=$(this);el.owlCarousel('destroy');el.owlCarousel({loop:_total_products>2,lazyLoad:true,autoWidth:false,nav:true,navText:['<i class="nki-arow-rounded-back" aria-hidden="true"></i>','<i class="nki-arow-rounded-next" aria-hidden="true"></i>'],items:_total_products===1?1:2,autoplay:_total_products>2,autoplayTimeout:5000,slideSpeed:8000,autoplayHoverPause:true,onInitialized:function(){el.find('img[class*=lazyload]').lazy($_lazy_opt);el.find('.owl-next').click(function(){el.find('img[class*=lazyload]').lazy($_lazy_opt);});el.find('.owl-prev').click(function(){el.find('img[class*=lazyload]').lazy($_lazy_opt);});},onDragged:function(){el.find('img[class*=lazyload]').lazy($_lazy_opt);}});});},complete:function(){},error:function(xhr){}});}
var RecentProduct={add_product_to_cookie:function(product_id,amount){if(amount>0){var recently_products=$.cookie("recently_products");if(recently_products==null||recently_products==""){recently_products=product_id;}
else{var products=recently_products.split(',');var index=$.inArray(product_id,products);if(index===-1){if(products.length<10){recently_products=product_id+','+recently_products;}else{products.splice(9,products.length-9);recently_products=product_id+','+products.join(',');}}
else{products.splice(index,1);if(products.length>0){if(products.length>=10){products.splice(9,products.length-9);}
recently_products=product_id+','+products.join(',');}else{recently_products=product_id;}}}
$.cookie("recently_products",recently_products,{expires:30,path:'/'});}}};function fn_load_other_dom_owlhome(){carouselCustom('.nk_banner_floor_carousel_2020_6items',null,false,true,false);carouselCustom('.nk_banner_floor_carousel_2020',3,false,true,false);carouselCustom('.new-carousel.owl-carousel.owl-theme',null,false,true,false);if(typeof $.fn.nk_houseware_best_selling_2020_wrapper!=='undefined'&&typeof $.fn.nk_houseware_best_selling_2020_wrapper==='function'){if($(".nk_houseware_best_selling_2020_wrapper .menu-item").length>0&&!$(".nk_houseware_best_selling_2020_wrapper").hasClass('js_done')){$(".nk_houseware_best_selling_2020_wrapper").nk_houseware_best_selling_2020_wrapper({});$(".nk_houseware_best_selling_2020_wrapper").addClass('js_done');}}};$(()=>{jQuery.event.special.touchstart={setup:function(_,ns,handle){if(ns.includes("noPreventDefault")){this.addEventListener("touchstart",handle,{passive:false});}else{this.addEventListener("touchstart",handle,{passive:true});}}};if($("#new-flash-sale").length===0){carouselCustom('.owl-carousel.nk-product-carousel-homepage');}});$(()=>{fn_load_other_dom_owlhome();carouselCustom('.owl-carousel.nk-banner-main',1,true,true,false);})