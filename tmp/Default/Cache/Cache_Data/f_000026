function validate_phone(){var loi=false;var f4=$('input[name=num]').val();if(f4==""||$.isNumeric(f4)==false){loi=true;}
if(loi==false){return true;}
return false;}
function validate_trade(){var loi=false;var f1=$('input[name=ht]').val();var f2=$('input[name=em]').val();var f4=$('input[name=num]').val();var re=/[A-Z0-9._%+-]+@[A-Z0-9.-]+.[A-Z]{2,4}/igm;if(f1==""){loi=true;}
if(f2==""){loi=true;}
if(!re.test(f2)){loi=true;}
if(f4==""||$.isNumeric(f4)==false){loi=true;}
if(loi==false){return true;}
return false;}
function fn_appointment(){if(validate_trade()){var firstname=$('input[name=ht]').val();var email=$('input[name=em]').val();var phone=$('input[name=num]').val();var b_address=$("#user_data_county_tt option:selected").text();var product_id=$('.product-id').val();var machine=$("#machine-type option:selected").val();var type=$("#model-title option:selected").val();var stt=$(".tinh-trang li.active a span.status-prd ").text();var price=$(".tinh-trang li.active a span.item-pr ").text();var mall_id=$(".ttnh option:selected").val();$.ajax({url:fn_url("products.save_tradeinsale"),method:"POST",data:{'firstname':firstname,'phone':phone,'email':email,'b_address':b_address,'product_id':product_id,'amount':1,'notes':"Thu cũ đổi mới - "+machine+" - "+type+" - Tình trạng "+stt+" - Giá thu dự kiến "+price+"",'mall_id':mall_id,"security_hash":Tygh.security_hash},dataType:"json",beforeSend:function(){$('#ajax_loading_box').show();$('#ajax_overlay').show();},success:function(msg){if(msg==1){$('.nk-form-trade-in-product').hide();$(".email-senter").text(email);$('.nk-form-trade-in-product-complete').show();}},error:function(XMLHttpRequest,textStatus,errorThrown){$('.nk-form-trade-in-product').hide();if(XMLHttpRequest.readyState==4){$('.nk-form-trade-in-product').hide();$('#ajax_loading_box').hide();$('#ajax_overlay').hide();TradeProductFuncFail();}else if(XMLHttpRequest.readyState==0){$('.nk-form-trade-in-product').hide();$('#ajax_loading_box').hide();$('#ajax_overlay').hide();TradeProductFuncFail();}else{$('.nk-form-trade-in-product').hide();$('#ajax_loading_box').hide();$('#ajax_overlay').hide();TradeProductFuncFail();}},complete:function(){$('#ajax_loading_box').hide();$('#ajax_overlay').hide();}});}else{return false;}}
function fn_load_center_choice(){if(mall=={}){$('.step-two .nk-box-head>span:eq(1)').hide();$('#nhan_hang_tt').hide();$('.step-two .nk-box-head>span:eq(0)>label').trigger("click");return false;}
