$.ajax({url:fn_url("products.coupon_hunter"),type:'POST',data:{'security_hash':Tygh.security_hash,'ProductID':_product_id,'CustomerPhone':_customerPhone,'CustomerMail':_CustomerMail},dataType:"json",beforeSend:function(){setTimeout(function(){$('#ajax_loading_box').show();},100);},success:function(data){var obj=JSON.parse(data);if(obj.status==true){$(".wrap_sanvoucher").hide();$(".type_coupon").text(obj.data.type);$(".phone_coupon").text(obj.data.phone);var saleoff=parseInt(obj.data.type);_price=parseInt(_price);var prc=(_price*(100-saleoff))/ 100;prc=Math.round(prc / 1000)*1000;prc=$.formatNum(prc);$(".price_apply_coupon").text(prc);$(".phone_hunter").val(obj.data.phone);$(".email_hunter").val(obj.data.mail);$('input[name=coupon_code]').val(obj.data.code);$(".wrap_info_coupon").show();hide_4last_number_phone();}else{if(obj.noti==500){$(".wrap_sanvoucher .input_code").prepend("<p class=\"phone_warning_required\">Đã hết mã</p>");}else{$(".wrap_sanvoucher .input_code").prepend("<p class=\"phone_warning_required\">Số điện thoại này đã được nhận mã trước đó</p>");}}
$('#ajax_loading_box').hide();},error:function(e){}});}
