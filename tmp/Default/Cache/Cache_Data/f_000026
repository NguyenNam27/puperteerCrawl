$('#ajax_loading_box').hide();},error:function(e){}});}
function fn_pdp_load_flash_sales(){if($('#product_data_check').length>0){const product_data_check=$('#product_data_check').val();const data_product=product_data_check.split('-'),nk_price_final=$('.nk-price-final'),layout=$('body').data('layout'),nk_shock_price=$('.nk-shock-price');var _data={};$.ajax({url:window.location.origin+'/esi.php?product_live='+product_data_check+'&layout='+layout,method:'get',dataType:' json',data:_data,beforeSend:function(){},success:function(data){if(data.json.data.products.length>0){const products=data.json.data.products[0];if(data_product[5].length>0&&parseInt(data_product[5])!=parseInt(products.price)){nk_price_final.data('price',products.price).html(formatCurrency(products.price));}
$('#warehouse-area').show();if((products.status=='A'||products.check_status==true)||parseInt(products.amount)>=parseInt(products.nk_check_total_amount)){if($('.btn-tra-gop-click').length>0){$('#buy_button_area').show().removeClass().addClass('product_info_buttons btn-2');}else{$('#buy_button_area').show().removeClass().addClass('product_info_buttons btn-1');}
if(data.json.status!=200){$('#flash-product').remove();$(".nks_fs_pdp").remove();}
